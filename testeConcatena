#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55781cacfdb0 .scope module, "ConcatenaBit" "ConcatenaBit" 2 1;
 .timescale 0 0;
v0x55781cad1350_0 .var "Entrada2Bits", 1 0;
v0x55781cad17b0_0 .var "Saida3Bits", 2 0;
    .scope S_0x55781cacfdb0;
T_0 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55781cad1350_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %load/vec4 v0x55781cad1350_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x55781cad17b0_0, 0, 3;
    %vpi_call 2 11 "$display", "Entrada2Bits: %b, Saida3Bits: %b", v0x55781cad1350_0, v0x55781cad17b0_0 {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
