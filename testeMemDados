:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55b58affb550 .scope module, "Teste_MemoriaDados" "Teste_MemoriaDados" 2 38;
 .timescale 0 0;
v0x55b58b012660_0 .var "DadoAEscrever", 7 0;
v0x55b58b012740_0 .net "DadoLido", 7 0, v0x55b58b0120b0_0;  1 drivers
v0x55b58b012810_0 .var "Endereco", 7 0;
v0x55b58b012910_0 .var "EscMem", 0 0;
v0x55b58b0129e0_0 .var "LerMem", 0 0;
v0x55b58b012ad0_0 .var "clock", 0 0;
S_0x55b58affb6e0 .scope module, "Memoria" "MemoriaDados" 2 44, 2 1 0, S_0x55b58affb550;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "Endereco";
    .port_info 1 /INPUT 8 "DadoEscrito";
    .port_info 2 /OUTPUT 8 "DadoLido";
    .port_info 3 /INPUT 1 "EscMem";
    .port_info 4 /INPUT 1 "LerMem";
    .port_info 5 /INPUT 1 "clock";
v0x55b58afbfcf0_0 .net "DadoEscrito", 7 0, v0x55b58b012660_0;  1 drivers
v0x55b58b0120b0_0 .var "DadoLido", 7 0;
v0x55b58b012190_0 .net "Endereco", 7 0, v0x55b58b012810_0;  1 drivers
v0x55b58b012250_0 .net "EscMem", 0 0, v0x55b58b012910_0;  1 drivers
v0x55b58b012310_0 .net "LerMem", 0 0, v0x55b58b0129e0_0;  1 drivers
v0x55b58b012420 .array "MemDados", 255 0, 7 0;
v0x55b58b0124e0_0 .net "clock", 0 0, v0x55b58b012ad0_0;  1 drivers
E_0x55b58aff9b50 .event negedge, v0x55b58b0124e0_0;
E_0x55b58afc0600 .event posedge, v0x55b58b0124e0_0;
    .scope S_0x55b58affb6e0;
T_0 ;
    %vpi_call 2 11 "$readmemb", "dados.dat", v0x55b58b012420, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000011111111 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x55b58affb6e0;
T_1 ;
    %wait E_0x55b58afc0600;
    %load/vec4 v0x55b58b012250_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55b58afbfcf0_0;
    %load/vec4 v0x55b58b012190_0;
    %pad/u 10;
    %ix/vec4 4;
    %store/vec4a v0x55b58b012420, 4, 0;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55b58affb6e0;
T_2 ;
    %wait E_0x55b58aff9b50;
    %load/vec4 v0x55b58b012310_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %load/vec4 v0x55b58b012190_0;
    %pad/u 10;
    %ix/vec4 4;
    %load/vec4a v0x55b58b012420, 4;
    %store/vec4 v0x55b58b0120b0_0, 0, 8;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55b58affb550;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b58b012ad0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b58b012910_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b58b0129e0_0, 0, 1;
    %pushi/vec4 8, 0, 8;
    %store/vec4 v0x55b58b012810_0, 0, 8;
    %pushi/vec4 15, 0, 8;
    %store/vec4 v0x55b58b012660_0, 0, 8;
    %vpi_call 2 52 "$display", "TESTE: Escrita e Leitura na Mem\303\263ria de Dados" {0 0 0};
    %vpi_call 2 53 "$display", "Tempo  Clock  EscMem  LerMem  Endereco  DadoAEscrever  DadoLido" {0 0 0};
    %vpi_call 2 54 "$monitor", " %0d       %d       %d      %d       %d         %d         %d", $time, v0x55b58b012ad0_0, v0x55b58b012910_0, v0x55b58b0129e0_0, v0x55b58b012810_0, v0x55b58b012660_0, v0x55b58b012740_0 {0 0 0};
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b58b012ad0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b58b012ad0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b58b012910_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b58b0129e0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b58b012ad0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b58b0129e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b58b012910_0, 0, 1;
    %end;
    .thread T_3;
